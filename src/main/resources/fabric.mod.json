{
    "schemaVersion": 1,
    "id": "hypixel-tab-completions",
    "version": "${version}",
    "name": "Hypixel Tab Completions",
    "description": "Adds tab completions for common commands on Hypixel.",
    "authors": [
        "Alex"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/hypixel-tab-completions",
        "sources": "https://github.com/OperationPotato/HypixelTabCompletions",
        "issues": "https://github.com/OperationPotato/HypixelTabCompletions/issues"
    },
    "icon": "assets/hypixel-tab-completions/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            {
                "value": "com.operationpotato.hypixeltabcompletions.HypixelTabCompletions",
                "adapter": "kotlin"
            }
        ]
    },
    "mixins": [
        "hypixel-tab-completions.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "minecraft": "~${minecraft_version}",
        "java": ">=${java_version}",
        "fabric-api": ">=${fabric_api_version}",
        "fabric-language-kotlin": ">=${fabric_kotlin_version}"
    }
}
